{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        },
        "logLevel": {
            "default": "Information",
            "Microsoft.SemanticKernel": "Debug",
            "AgenticAI": "Debug"
        }
    },
    "functionTimeout": "00:10:00",
    "extensions": {
        "blobs": {
            "maxDegreeOfParallelism": 4
        }
    }
}